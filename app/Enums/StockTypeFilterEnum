<?php 

namespace App\Enums;

enum StockTypeFilterEnum: int
{ 
    case image = 0;
    case vector = 1;
    case mockup = 2; 
    case video = 3;
    case icon = 4;
    case psd = 5; 

    public function getStockTypeDescription(): string{
        return match($this){ 
            self::image => 'image',
            self::vector => 'vector',
            self::mockup => 'mockup',
            self::video => 'video',
            self::icon => 'psd', 
        };
    }
} 